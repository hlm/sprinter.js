{
    "name": "sprinter",
    "version": "1.0.0",
    "author": {
        "name" : "Matthew Taylor",
        "email" : "rhyolight@gmail.com",
        "url" : "http://about.me/rhyolight"
    },
    "description": "Utilities for operating on issue trackers of several repositories at once.",
    "main": "sprinter.js",
    "scripts": {
        "test": "istanbul cover _mocha -- --recursive -R spec && istanbul report text-summary > coverage/summary.txt && cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
    },
    "bin" : "./bin/cli.js",
    "keywords": [
        "github", "issues", "milestones", "agile", "scrum"
    ],
    "contributors": [{
            "name": "Matt Taylor",
            "email": "rhyolight@gmail.com"
    }],
    "repository": {
        "type": "git",
        "url": "git://github.com/rhyolight/sprinter.js.git"
    },
    "bugs": {
        "url": "https://github.com/rhyolight/sprinter.js/issues"
    },
    "dependencies": {
        "colors"     : "0.6.2",
        "moment"     : "2.5.1",
        "minimist"   : "0.0.8",
        "underscore" : "1.6.0",
        "async"      : "0.4.0",
        "github"     : "0.1.15",
        "js-yaml"    : "3.0.2"
    },
    "devDependencies": {
        "proxyquire"    : "0.5.x",
        "mocha"         : "1.12.x",
        "istanbul"      : "0.1.x",
        "sinon"         : "1.7.x",
        "chai"          : "1.9.x",
        "coveralls"     : "2.10.0"
    },
    "license": "MIT",
    "engines": {
        "node": ">=0.10"
    }
}
